<?xml version="1.0" encoding="ISO-8859-1"?>	

<!DOCTYPE rdf:RDF [
	<!ENTITY owl "http://www.w3.org/2002/07/owl#" >
	<!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
	<!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
	<!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
	<!ENTITY XMLSchemaMetamodelOntology "http://www.semanticweb.org/ontologies/XMLSchemaOntologies/XMLSchemaMetamodelOntology.owl#" >
]>
	
<rdf:RDF 
     xmlns="http://www.semanticweb.org/ontologies/XMLSchemaOntologies/simpledc.owl#"
	 xml:base="http://www.semanticweb.org/ontologies/XMLSchemaOntologies/simpledc.owl"
	 xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
	 xmlns:owl="http://www.w3.org/2002/07/owl#"
	 xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
	 xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
	 xmlns:XSDMetamodelOntology="http://www.semanticweb.org/ontologies/XMLSchemaOntologies/XMLSchemaMetamodelOntology.owl#">
	
	<owl:Ontology rdf:about="http://www.semanticweb.org/ontologies/XMLSchemaOntologies/simpledc.owl">
		<owl:versionIRI rdf:resource="http://www.semanticweb.org/ontologies/XMLSchemaOntologies/simpledc:1.0.0.owl"/>
		<owl:imports rdf:resource="http://www.semanticweb.org/ontologies/XMLSchemaOntologies/XMLSchemaMetamodelOntology:1.0.0.owl"/>
	</owl:Ontology>
	
	
	<!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->	
	
	<!-- http://www.semanticweb.org/ontologies/XMLSchemaOntologies/simpledc.owl#simpledc-Schema -->
	
	<owl:Class rdf:about="http://www.semanticweb.org/ontologies/XMLSchemaOntologies/simpledc.owl#simpledc-Schema">
		<rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/XMLSchemaOntologies/XMLSchemaMetamodelOntology.owl#Schema"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="http://www.semanticweb.org/ontologies/XMLSchemaOntologies/XMLSchemaMetamodelOntology.owl#attributeFormDefault_Schema_String"/>
				<owl:hasValue rdf:datatype="&xsd;string">unqualified</owl:hasValue>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="http://www.semanticweb.org/ontologies/XMLSchemaOntologies/XMLSchemaMetamodelOntology.owl#elementFormDefault_Schema_String"/>
				<owl:hasValue rdf:datatype="&xsd;string">qualified</owl:hasValue>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/ontologies/XMLSchemaOntologies/XMLSchemaMetamodelOntology.owl#contains_Schema_Import"/>
                <owl:allValuesFrom rdf:resource="http://www.semanticweb.org/ontologies/XMLSchemaOntologies/simpledc.owl#Import2-Import_simpledc-Schema"/>
            </owl:Restriction>
        </rdfs:subClassOf>
		<rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/ontologies/XMLSchemaOntologies/XMLSchemaMetamodelOntology.owl#contains_Schema_Annotation"/>
                <owl:allValuesFrom rdf:resource="http://www.semanticweb.org/ontologies/XMLSchemaOntologies/simpledc.owl#Annotation1-Annotation_simpledc-Schema"/>
            </owl:Restriction>
        </rdfs:subClassOf>
		<rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/ontologies/XMLSchemaOntologies/XMLSchemaMetamodelOntology.owl#contains_Schema_Element"/>
                <owl:allValuesFrom rdf:resource="http://www.semanticweb.org/ontologies/XMLSchemaOntologies/simpledc.owl#simpledc-Element_simpledc-Schema"/>
            </owl:Restriction>
        </rdfs:subClassOf>
	</owl:Class>	
	
	<!-- http://www.semanticweb.org/ontologies/XMLSchemaOntologies/simpledc.owl#Import2-Import_simpledc-Schema -->
	
	<owl:Class rdf:about="http://www.semanticweb.org/ontologies/XMLSchemaOntologies/simpledc.owl#Import2-Import_simpledc-Schema">
		<rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/XMLSchemaOntologies/XMLSchemaMetamodelOntology.owl#Import"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="http://www.semanticweb.org/ontologies/XMLSchemaOntologies/XMLSchemaMetamodelOntology.owl#namespace_Import_String"/>
				<owl:hasValue rdf:datatype="&xsd;string">http://purl.org/dc/elements/1.1/</owl:hasValue>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="http://www.semanticweb.org/ontologies/XMLSchemaOntologies/XMLSchemaMetamodelOntology.owl#schemaLocation_Import_String"/>
				<owl:hasValue rdf:datatype="&xsd;string">dc.xsd</owl:hasValue>
			</owl:Restriction>
		</rdfs:subClassOf>
	</owl:Class>	
	
	<!-- http://www.semanticweb.org/ontologies/XMLSchemaOntologies/simpledc.owl#Annotation1-Annotation_simpledc-Schema -->
	
	<owl:Class rdf:about="http://www.semanticweb.org/ontologies/XMLSchemaOntologies/simpledc.owl#Annotation1-Annotation_simpledc-Schema">
		<rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/XMLSchemaOntologies/XMLSchemaMetamodelOntology.owl#Annotation"/>
		<rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/ontologies/XMLSchemaOntologies/XMLSchemaMetamodelOntology.owl#contains_Annotation_Documentation"/>
                <owl:allValuesFrom rdf:resource="http://www.semanticweb.org/ontologies/XMLSchemaOntologies/simpledc.owl#Documentation1-Documentation_Annotation1-Annotation_simpledc-Schema"/>
            </owl:Restriction>
        </rdfs:subClassOf>
	</owl:Class>	
	
	<!-- http://www.semanticweb.org/ontologies/XMLSchemaOntologies/simpledc.owl#Documentation1-Documentation_Annotation1-Annotation_simpledc-Schema -->
	
	<owl:Class rdf:about="http://www.semanticweb.org/ontologies/XMLSchemaOntologies/simpledc.owl#Documentation1-Documentation_Annotation1-Annotation_simpledc-Schema">
		<rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/XMLSchemaOntologies/XMLSchemaMetamodelOntology.owl#Documentation"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="http://www.semanticweb.org/ontologies/XMLSchemaOntologies/XMLSchemaMetamodelOntology.owl#xml:lang_Documentation_String"/>
				<owl:hasValue rdf:datatype="&xsd;string">en</owl:hasValue>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="http://www.semanticweb.org/ontologies/XMLSchemaOntologies/XMLSchemaMetamodelOntology.owl#any_Documentation_String"/>
				<owl:hasValue rdf:datatype="&xsd;string">
      Simple DC container XML Schema
 
      Created 2008-02-11

      Created by 

      Tim Cole (t-cole3@uiuc.edu)
      Tom Habing (thabing@uiuc.edu)
      Jane Hunter (jane@dstc.edu.au)
      Pete Johnston (p.johnston@ukoln.ac.uk),
      Carl Lagoze (lagoze@cs.cornell.edu)

      This schema declares a container element for a Simple DC application. 
      
      The declaration of the simpledc element uses the dc:elementContainer
      complexType. 

      Note that this schema does not define a target namespace. The expectation is that
      the simpledc element is assigned to a namespace by an application schema 
      which includes this schema.
      
    </owl:hasValue>
			</owl:Restriction>
		</rdfs:subClassOf>
	</owl:Class>	
	
	<!-- http://www.semanticweb.org/ontologies/XMLSchemaOntologies/simpledc.owl#simpledc-Element_simpledc-Schema -->
	
	<owl:Class rdf:about="http://www.semanticweb.org/ontologies/XMLSchemaOntologies/simpledc.owl#simpledc-Element_simpledc-Schema">
		<rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/XMLSchemaOntologies/XMLSchemaMetamodelOntology.owl#Element"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="http://www.semanticweb.org/ontologies/XMLSchemaOntologies/XMLSchemaMetamodelOntology.owl#name_Element_String"/>
				<owl:hasValue rdf:datatype="&xsd;string">simpledc</owl:hasValue>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/ontologies/XMLSchemaOntologies/XMLSchemaMetamodelOntology.owl#type_Element_Type"/>
                <owl:allValuesFrom rdf:resource="http://www.semanticweb.org/ontologies/XMLSchemaOntologies/http://purl.org/dc/elements/1.1/.owl#elementContainer-Type_http://purl.org/dc/elements/1.1/-Schema"/>
            </owl:Restriction>
        </rdfs:subClassOf>
	</owl:Class>	
</rdf:RDF>
